package day08;

public class Animal{
  
  private int feet;
  private String name;

  public Animal(int feet , String name){
    this.feet = feet;
    this.name = name;
  }

  // Getter & Setter
  public int getFeet(){
    return feet;
  }

  public void setFeet(int feet){
    this.feet = feet;
  }

  public String getName(){
    return name;
  }

  public void setName(String name){
    this.name = name;
  }

  public void move(){
    System.out.println("會動");
  }
}

package 08;
public class Bird extends Animal{

}
