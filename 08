package day08;

public class Animal{ // 動物(父類別)
  
  private int feet;
  private String name;

  public Animal(int feet , String name){
    this.feet = feet;
    this.name = name;
  }

  // Getter & Setter
  public int getFeet(){
    return feet;
  }

  public void setFeet(int feet){
    this.feet = feet;
  }

  public String getName(){
    return name;
  }

  public void setName(String name){
    this.name = name;
  }

  public void move(){
    System.out.println("會動");
  }
}

package 08;
public class Bird extends Animal{ // 鳥(父/子類別)
  public Bird(int feet, String name){
    super(feet, name);
  }

  @override
  public void move(){
    System.out.println("會飛");
  }

  public void eat(){
    System.out.println("小麥");
  }
}

package day08;
public class Lion extends Animal{ // 獅子(子類別)

  public Lion(int feet, String name){
    super(feet, name);
  }

  @override
  public void move(){
    System.out.println("會跑");
  }

  public void eat(){
    System.out.println("肉");
  }
}

package day08;
public class Ostrich extends Bird{ // 鴕鳥(子類別)

  public Lion(int feet, String name){
    super(feet, name);
  }

  @override
  public void move(){
    System.out.println("會跑");
  }

  public void eat(){
    System.out.println("肉");
  }
}
