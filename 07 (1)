package day07;
public class Machine{
  public void power(){
    System.out.println("吃電")
  }
}

public class Computer extends Machine{ // 繼承(父子)
  public void draw(){
    System.out.println("繪圖電腦");
  }

public class Phone extends Computer{ // 繼承(父子)
  public void call(){
    System.out.println("打電話")
  }
}
